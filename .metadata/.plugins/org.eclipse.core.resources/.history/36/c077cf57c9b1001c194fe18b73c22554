class Medicine
{
  //code to build Medicine class
  String medicineName;
  int batchNo;
  String disease ;
  int price;
  
  public String getDisease()
  {
    return disease;
  }
  public int getPrice()
  {
    return price;
  }
  Medicine(String medicineName,int batchNo,String disease,int price)
  {
    this.medicineName=medicineName;
    this.batchNo=batchNo;
    this.disease=disease;
    this.price=price;
  }
  
}
            